{
  "expo": {
    "name": "Aureynx Wildlife Corridors",
    "slug": "aureynx-wildlife-corridors",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Aureynx_Logo.png",
    "scheme": "aureynx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aureynx.wildlifecorridors",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show wildlife corridor data and nearby alerts.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to track patrol routes and wildlife movements.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of wildlife sightings.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to attach photos to wildlife reports.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Aureynx_Logo.png",
        "backgroundColor": "#2E5D45"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "package": "com.aureynx.wildlifecorridors"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Aureynx_Logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-local-authentication",
      "expo-image-picker",
      "expo-location",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Aureynx_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2E5D45"
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMAPBOX_MAPS_DOWNLOAD_TOKEN": "pk.eyJ1IjoiamFtbXktbCIsImEiOiJjbWd0YWRmNnAwMjZwMmpyN252a2d2ajd1In0.M0mjFFqLPLGPfz2mEFNBKA"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d51b5b66-0676-476d-bb34-37e51bcf13d6"
      },
      "apiUrl": "https://wildlife-project-backend.onrender.com",
      "environment": "production"
    },
    "owner": "jammy-b",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/d51b5b66-0676-476d-bb34-37e51bcf13d6"
    }
  }
}
