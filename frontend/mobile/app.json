{
  "expo": {
    "name": "Aureynx Wildlife Corridors",
    "slug": "aureynx-wildlife-corridors",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Aureynx_Logo.png",
    "scheme": "aureynx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Aureynx_Logo.png",
        "backgroundColor": "#3B6B3A"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Aureynx_Logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-local-authentication",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Aureynx_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#3B6B3A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
